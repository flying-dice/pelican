    "license": "ISC",
    "devDependencies": {
        "concurrently": "^9.1.2",
        "lua-fmt": "^2.6.0",
        "lua-types": "^2.13.1",
        "prettier": "^3.5.3",
        "typedoc": "^0.28.5",
        "typescript": "^5.8.3",
        "typescript-to-lua": "^1.31.1"
    }
}
